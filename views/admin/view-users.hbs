<section class="view-Users">
    <div class="container">
        <h2>Users</h2>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">No</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col"></th>
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                    <td scope="row">*</td>
                    <td>{{this.Name}}</td>
                    <td>{{this.Email}}</td>
                    <td>
                        <button href="" class="btn btn-danger">Block</button>
                    </td>
                </tr>
                {{/each}}
            
            </tbody>
        </table>
    </div>
</section>